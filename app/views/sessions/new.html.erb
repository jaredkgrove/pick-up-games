<div class="sign-up">
  <%= link_to('Log in with Facebook!', '/auth/facebook') %> <br>
  <div> or</div>
  <p> <strong> Login with your Email Address </strong></p>
  <%= form_for :player, url: '/sessions' do |f| %>
    <%= f.text_field :email, placeholder: "Email" %>
    <%= f.password_field :password, placeholder: "Password" %>
    <%= f.submit "Login" %>
  <% end %>
</div>